  </Version>
  <Version rdf:about="http://purl.org/NET/cpan-uri/dist/HTML-Microformats/v_0-00_09">
    <dcs:changeset>
      <rdf:Description>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">XOXO support.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:comment xml:lang="en">Not technically a microformat.</rdfs:comment>
            <rdfs:label xml:lang="en">OpenURL COinS support.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">figure support.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <rdf:Description>
            <rdfs:label xml:lang="en">adr RDF output now better differentiates between the addresses and the places they represent.</rdfs:label>
          </rdf:Description>
        </dcs:item>
        <dcs:item>
          <rdf:Description>
            <rdfs:label xml:lang="en">Document the rel-* microformat implementations and OpenURL COinS.</rdfs:label>
          </rdf:Description>
        </dcs:item>
        <dcs:item>
          <rdf:Description>
            <rdfs:label xml:lang="en">Document the ISO8601 datetime parser.</rdfs:label>
          </rdf:Description>
        </dcs:item>
        <dcs:versus rdf:resource="http://purl.org/NET/cpan-uri/dist/HTML-Microformats/v_0-00_08"/>
      </rdf:Description>
    </dcs:changeset>
