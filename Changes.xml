  <Version rdf:about="http://purl.org/NET/cpan-uri/dist/HTML-Microformats/v_0-00_08">
    <dcs:changeset>
      <rdf:Description>
        <dcs:item>
          <rdf:Description>
            <rdfs:comment xml:lang="en">This means that two runs of HTML::Microformats will result in different identifiers.</rdfs:comment>
            <rdfs:label xml:lang="en">Introduce a context-unique component to blank node identifiers.</rdfs:label>
          </rdf:Description>
        </dcs:item>
        <dcs:item>
          <rdf:Description>
            <rdfs:label xml:lang="en">Distinguish between base URI and document/context URI.</rdfs:label>
          </rdf:Description>
        </dcs:item>
        <dcs:versus rdf:resource="http://purl.org/NET/cpan-uri/dist/HTML-Microformats/v_0-00_07"/>
        <rdfs:comment xml:lang="en">These changes are for the benefit of XML-Atom-Microformats.</rdfs:comment>
      </rdf:Description>
    </dcs:changeset>
    <dc:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2010-04-29</dc:issued>
