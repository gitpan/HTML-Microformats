    <dc:creator rdf:resource="http://tobyinkster.co.uk/#i"/>
    <dc:description xml:lang="en">Revision history for Perl extension HTML::Microformats.</dc:description>
    <dc:subject rdf:resource="http://purl.org/NET/cpan-uri/dist/HTML-Microformats/project"/>
    <dc:title>Changes</dc:title>
  </rdf:Description>
  <Version rdf:about="http://purl.org/NET/cpan-uri/dist/HTML-Microformats/v_0-00_00">
    <dc:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2010-02-19</dc:issued>
    <file-release rdf:resource="http://backpan.cpan.org/authors/id/T/TO/TOBYINK/HTML-Microformats-0.00_00.tar.gz"/>
    <revision rdf:datatype="http://www.w3.org/2001/XMLSchema#string">0.00_00</revision>
    <rdfs:comment xml:lang="en">Original version, forked from Swignition. Supports hCard, adr, geo and rel-tag.</rdfs:comment>
  </Version>
  <Version rdf:about="http://purl.org/NET/cpan-uri/dist/HTML-Microformats/v_0-00_01">
    <dcs:changeset>
      <rdf:Description>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">Figure out which hCard on a page is the 'representative' one (for the purposes of XFN).</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">XFN support.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <rdf:Description>
            <rdfs:label xml:lang="en">Copyright statements now reflect the fact that much of this code comes from 2008.</rdfs:label>
          </rdf:Description>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">Draft 'species' microformat support.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <rdf:Description>
            <rdfs:label xml:lang="en">Improve hCard type+value property parsing.</rdfs:label>
          </rdf:Description>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">Draft 'hMeasure' microformat support.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <rdf:Description>
            <rdfs:label xml:lang="en">Document HTML::Microformats::hCard.</rdfs:label>
          </rdf:Description>
        </dcs:item>
        <dcs:versus rdf:resource="http://purl.org/NET/cpan-uri/dist/HTML-Microformats/v_0-00_00"/>
      </rdf:Description>
    </dcs:changeset>
    <dc:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2010-02-24</dc:issued>
    <file-release rdf:resource="http://backpan.cpan.org/authors/id/T/TO/TOBYINK/HTML-Microformats-0.00_01.tar.gz"/>
    <revision rdf:datatype="http://www.w3.org/2001/XMLSchema#string">0.00_01</revision>
    <rdfs:comment xml:lang="en">Adds XFN, species and hMeasure.</rdfs:comment>
  </Version>
  <Version rdf:about="http://purl.org/NET/cpan-uri/dist/HTML-Microformats/v_0-00_02">
    <dcs:changeset>
      <rdf:Description>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">rel-license support.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">rel-enclosure support.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">Support for hAtom and hNews.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <rdf:Description>
            <rdfs:label xml:lang="en">Document geo, adr, XFN and hAtom support.</rdfs:label>
          </rdf:Description>
        </dcs:item>
        <dcs:item>
          <rdf:Description>
            <rdfs:label xml:lang="en">Split out some common rel-X parsing into HTML::Microformats::_rel.</rdfs:label>
          </rdf:Description>
        </dcs:item>
        <dcs:versus rdf:resource="http://purl.org/NET/cpan-uri/dist/HTML-Microformats/v_0-00_01"/>
      </rdf:Description>
    </dcs:changeset>
    <dc:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2010-03-01</dc:issued>
    <file-release rdf:resource="http://backpan.cpan.org/authors/id/T/TO/TOBYINK/HTML-Microformats-0.00_02.tar.gz"/>
    <revision rdf:datatype="http://www.w3.org/2001/XMLSchema#string">0.00_02</revision>
    <rdfs:comment xml:lang="en">Adds rel-license, rel-enclosure, hAtom and hNews.</rdfs:comment>
  </Version>
  <Version rdf:about="http://purl.org/NET/cpan-uri/dist/HTML-Microformats/v_0-00_03">
    <dcs:changeset>
      <rdf:Description>
        <dcs:item>
          <rdf:Description>
            <rdfs:label xml:lang="en">Rename _base.pm to BASE.pm; _simple_parser.pm to Mixin/Parser.pm; _simple_rdf.pm to Mixin/RDF.pm; _rel.pm to BASEREL.pm.</rdfs:label>
          </rdf:Description>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">hCalendar support.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <rdf:Description>
            <rdfs:label xml:lang="en">Document BASE microformat class.</rdfs:label>
          </rdf:Description>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">RecurringDateTime datatype.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:versus rdf:resource="http://purl.org/NET/cpan-uri/dist/HTML-Microformats/v_0-00_02"/>
      </rdf:Description>
    </dcs:changeset>
    <dc:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2010-03-09</dc:issued>
    <file-release rdf:resource="http://backpan.cpan.org/authors/id/T/TO/TOBYINK/HTML-Microformats-0.00_03.tar.gz"/>
    <revision rdf:datatype="http://www.w3.org/2001/XMLSchema#string">0.00_03</revision>
    <rdfs:comment xml:lang="en">Adds hCalendar.</rdfs:comment>
  </Version>
  <Version rdf:about="http://purl.org/NET/cpan-uri/dist/HTML-Microformats/v_0-00_04">
    <dcs:changeset>
      <rdf:Description>
        <dcs:item>
          <dcs:Bugfix>
            <rdfs:label xml:lang="en">Problems parsing hCard org properties resolved.</rdfs:label>
          </dcs:Bugfix>
        </dcs:item>
        <dcs:item>
          <dcs:Bugfix>
            <rdfs:label xml:lang="en">hMeasure.pm wasn't inheriting properly from BASE.pm.</rdfs:label>
          </dcs:Bugfix>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">Implement class=value-title support.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">Implement implied date for hCalendar dtend and hAtom updated properties.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <rdf:Description>
            <rdfs:label xml:lang="en">Improvements in natural language date parsing.</rdfs:label>
          </rdf:Description>
        </dcs:item>
        <dcs:item>
          <dcs:Bugfix>
            <rdfs:label xml:lang="en">UTF8 fixes.</rdfs:label>
          </dcs:Bugfix>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">hResume support.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">hReview and xFolk support.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:versus rdf:resource="http://purl.org/NET/cpan-uri/dist/HTML-Microformats/v_0-00_03"/>
      </rdf:Description>
    </dcs:changeset>
