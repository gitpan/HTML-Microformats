my:v_0-00_07

	a               :Version ;
	dc:issued       "2010-04-28"^^xsd:date ;
	:revision       "0.00_07"^^xsd:string ;
	:file-release   <http://backpan.cpan.org/authors/id/T/TO/TOBYINK/HTML-Microformats-0.00_07.tar.gz> ;
	dcs:changeset [
			dcs:versus my:v_0-00_06 ;
